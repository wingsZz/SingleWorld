<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soul.android.single.world">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".camera.LiveCameraActivity" />
        <activity android:name=".player.TextureVideoPlayerActivity" />
        <activity android:name=".opengl.OpenGLDrawActivity" />
        <activity android:name=".alpha.AlphaMovieActivity" />
        <activity android:name=".codec.decode.DecodeActivity" />
        <activity android:name=".opengl.yuv.YUVConvertActivity" />
        <activity android:name=".opengl.vao.VaoActivity" />
        <activity android:name=".opengl.fbo.FboActivity" />
    </application>

</manifest>